    <AssemblyName>ChrisKaczor.HomeMonitor.Weather.Service</AssemblyName>
    <RootNamespace>ChrisKaczor.HomeMonitor.Weather.Service</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Data\Resources\CreateReading.sql" />
